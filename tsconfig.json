{
    "compilerOptions": {
        "outDir": "./dist/",
        "noImplicitAny": false,
        "noImplicitOverride": true,
        "module": "ES2022",
        "target": "ES2022",
        "jsx": "react",
        "allowJs": true,
        "moduleResolution": "node",
        "importHelpers": true,
        "allowSyntheticDefaultImports": true,
        "esModuleInterop": true,
        "baseUrl": ".",
        "paths": {
        "~/*": ["./*"]
        }
    }
}

